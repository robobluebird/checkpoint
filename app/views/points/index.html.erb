<p id="notice"><%= notice %></p>

<h1>Points</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Datetime</th>
      <th>Message</th>
      <th>Complete</th>
      <th>Creator</th>
      <th>Consumer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @points.each do |point| %>
      <tr>
        <td><%= point.latitude %></td>
        <td><%= point.longitude %></td>
        <td><%= point.datetime %></td>
        <td><%= point.message %></td>
        <td><%= point.complete %></td>
        <td><%= point.creator %></td>
        <td><%= point.consumer %></td>
        <td><%= link_to 'Show', point %></td>
        <td><%= link_to 'Edit', edit_point_path(point) %></td>
        <td><%= link_to 'Destroy', point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Point', new_point_path %>
